<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Trace Link Category"
            id="se.gu.featuredashboard.ui.commands.traceLinkCategory">
      </category>
      <command
            name="Create Trace Link Command"
            categoryId="se.gu.featuredashboard.ui.commands.traceLinkCategory"
            id="se.gu.featuredashboard.ui.commands.createTraceLinkCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="se.gu.featuredashboard.ui.commands.createTraceLinkCommand"
            class="se.gu.featuredashboard.ui.handlers.CreateTraceLinkHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="se.gu.featuredashboard.ui.commands.createTraceLinkCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="FeatureDashboard"
               mnemonic="M"
               id="se.gu.featuredashboard.ui.menus.featureDashboardMenu">
            <command
                  commandId="se.gu.featuredashboard.ui.commands.createTraceLinkCommand"
                  mnemonic="S"
                  id="se.gu.featuredashboard.ui.menus.createTraceLink">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="se.gu.featuredashboard.ui.toolbars.Example1Toolbar">
            <command
                  commandId="se.gu.featuredashboard.ui.commands.createTraceLinkCommand"
                  icon="icons/featureDashboard_icon.png"
                  tooltip="run feature location dashboard for example1"
                  id="se.gu.featuredashboard.ui.toolbars.createTraceLinkCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   
   <extension
      point = "org.eclipse.ui.preferencePages">
       <page id="se.gu.featuredashboard.ui.mainPreferences.page"
	    class="se.gu.featuredashboard.ui.preferences.MainPreferences"
            name="Feature Dashboard">
       </page>
   </extension>
   <extension
      point = "org.eclipse.ui.preferencePages">
       <page id="se.gu.featuredashboard.ui.regexPreference.page"
	    class="se.gu.featuredashboard.ui.preferences.RegexPreferences"
	    category = "se.gu.featuredashboard.ui.mainPreferences.page"
            name="Regex Patterns">
       </page>
   </extension>
   
    <extension
         point="org.eclipse.ui.views">
      <category
            name="FeatureDashboard"
            id="se.gu.featuredashboard.ui.views">
      </category>
      <view
            name="Main View"
            icon="icons/featureDashboard_icon.png"
            category="se.gu.featuredashboard.ui.views"
            class="se.gu.featuredashboard.ui.views.MainView"
            id="se.gu.featuredashboard.ui.views.MainView">
      </view>
      </extension>
      
      <extension
         point="org.eclipse.ui.views">
      <category
            name="FeatureDashboard"
            id="se.gu.featuredashboard.ui.views">
      </category>
      <view
            name="Feature List View"
            icon="icons/featureDashboard_icon.png"
            category="se.gu.featuredashboard.ui.views"
            class="se.gu.featuredashboard.ui.views.FeatureListView"
            id="se.gu.featuredashboard.ui.views.FeatureListView">
      </view>
      </extension>
      
      <extension
         point="org.eclipse.ui.views">
      <category
            name="FeatureDashboard"
            id="se.gu.featuredashboard.ui.views">
      </category>
      <view
            name="Feature-to-File View"
            icon="icons/featureDashboard_icon.png"
            category="se.gu.featuredashboard.ui.views"
            class="se.gu.featuredashboard.ui.views.FeatureFileView"
            id="se.gu.featuredashboard.ui.views.FeatureFileView">
      </view>
      </extension>
      
      <extension
         point="org.eclipse.ui.views">
      <category
            name="FeatureDashboard"
            id="se.gu.featuredashboard.ui.views">
      </category>
      <view
            name="Feature-to-Folder View"
            icon="icons/featureDashboard_icon.png"
            category="se.gu.featuredashboard.ui.views"
            class="se.gu.featuredashboard.ui.views.FeatureFolderView"
            id="se.gu.featuredashboard.ui.views.FeatureFolderView">
      </view>
      </extension>
      
      <extension
         point="org.eclipse.ui.views">
      <category
            name="FeatureDashboard"
            id="se.gu.featuredashboard.ui.views">
      </category>
      <view
            name="Feature Metrics View"
            icon="icons/featureDashboard_icon.png"
            category="se.gu.featuredashboard.ui.views"
            class="se.gu.featuredashboard.ui.views.FeatureMetricsView"
            id="se.gu.featuredashboard.ui.views.FeatureMetricsView">
      </view>
      </extension>
      
</plugin>
